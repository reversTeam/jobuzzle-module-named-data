#import (Config.path +'/Modules/Data/Bundle/Data/Entity/DataDataMainEntity') = DataDataMainEntity;

class NamedDataDataMainEntity extends DataDataMainEntity {

	_type = 'NamedData';
	_templateTimeline = 'nameddata_data_timeline_template';

	#default get, set for _name;

	initialize : function () {
		_super();

		this._name = undefined;
	}

	hydrate : function (datas) {
		_super(datas);

		if (datas.name)
			this.setName(datas.name);
	}

}

#export NamedDatDataMainEntity;